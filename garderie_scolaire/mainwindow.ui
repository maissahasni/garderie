       <string notr="true">QWidget {
    background-color: transparent; /* Rendre le fond transparent */
}

/* Le cadre du widget d'onglet */
QTabWidget::pane {
    border-top: 2px solid #C2C7CB;
    background-color: transparent; /* Rendre le milieu transparent */
}

/* La barre d'onglet */
QTabWidget::tab-bar {
    alignment: center;
    background-color: #003e46;
    border-top-left-radius: 5px;
    border-top-right-radius: 5px;
}

/* Les onglets non sélectionnés */
QTabBar::tab {
    font-weight: bold;
    border: 2px solid white;
    border-top-left-radius: 5px;
    border-top-right-radius: 5px;
    padding: 6px;
    color: white;
    height: 25px;
    width: 120px;
    background: transparent; /* Rendre le fond transparent */
}

/* L'onglet sélectionné */
QTabBar::tab:selected {
    background-color: white;
    color: #003e46;
}

/* L'onglet non sélectionné au survol */
QTabBar::tab:!selected:hover {
    background-color: #FCB0B3;
}</string>
